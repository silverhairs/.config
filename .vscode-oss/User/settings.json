/**
Requires the following fonts to be installed in the system:
__Some of them might be available in github.com/silverhairs/fonts__
- Geist Mono
- Operator Mono
- SpaceMono Nerd Font
- Space Mono Powerline
- Dank Mono
- MonoLisa
- SF Mono
*/
{
  "background.fullscreen": {
    "images": [
      "https://raw.githubusercontent.com/silverhairs/.config/refs/heads/main/kitty/assets/bochi.jpg"
    ],
    "opacity": 0.85,
    "size": "cover",
    "position": "center",
    "interval": 0
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.inlayHints.enabled": "off",
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.minimap.showRegionSectionHeaders": true,
    "editor.minimap.showMarkSectionHeaders": true,
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit",
      "source.fixAll": "explicit"
    }
  },

  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.screenReaderAnnounceInlineSuggestion": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "cmake.configureOnOpen": false,
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  "dart.openDevTools": "flutter",
  "debug.allowBreakpointsEverywhere": true,
  "debug.console.fontFamily": "'Iosevka','JetBrains Mono','Geist Mono', SF Mono",
  "debug.console.fontSize": 17,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 4,
  "editor.cursorBlinking": "expand",
  "editor.fontFamily": "'Operator Mono','Geist Mono','JetBrains Mono', DM Mono, MonoLisa, Geist Mono, 'SF Mono','Dank Mono','SpaceMono Nerd Font', MonoLisa,  'Space Mono for Powerline', Nova",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "debug.toolBarLocation": "commandCenter",
  "editor.suggestSelection": "first",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Comment",
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "comment.line.double-slash",
          "punctuation.definition.comment",
          "keyword",
          "storage.modifier"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "constant",
          "constant.character",
          "constant.character.escape",
          "constant.numeric",
          "constant.numeric.integer",
          "constant.numeric.float",
          "constant.numeric.hex",
          "constant.numeric.octal",
          "constant.other",
          "constant.regexp",
          "constant.rgb-value",
          "emphasis",
          "entity",
          "entity.name",
          "entity.name.class",
          "entity.name.function",
          "entity.name.method",
          "entity.name.section",
          "entity.name.selector",
          "entity.name.tag",
          "entity.name.type",
          "entity.other",
          "entity.other.attribute-name",
          "entity.other.inherited-class",
          "invalid",
          "invalid.deprecated",
          "invalid.illegal",
          "keyword",
          "keyword.control",
          "keyword.operator",
          "keyword.operator.new",
          "keyword.operator.assignment",
          "keyword.operator.arithmetic",
          "keyword.operator.logical",
          "keyword.other",
          "markup",
          "markup.bold",
          "markup.changed",
          "markup.deleted",
          "markup.heading",
          "markup.inline.raw",
          "markup.inserted",
          "markup.italic",
          "markup.list",
          "markup.list.numbered",
          "markup.list.unnumbered",
          "markup.other",
          "markup.quote",
          "markup.raw",
          "markup.underline",
          "markup.underline.link",
          "meta",
          "meta.block",
          "meta.cast",
          "meta.class",
          "meta.function",
          "meta.function-call",
          "meta.preprocessor",
          "meta.return-type",
          "meta.selector",
          "meta.tag",
          "meta.type.annotation",
          "meta.type",
          "punctuation.definition.string.begin",
          "punctuation.definition.string.end",
          "punctuation.separator",
          "punctuation.separator.continuation",
          "punctuation.terminator",
          "storage",
          "storage.modifier",
          "storage.type",
          "string",
          "string.interpolated",
          "string.other",
          "string.quoted",
          "string.quoted.double",
          "string.quoted.other",
          "string.quoted.single",
          "string.quoted.triple",
          "string.regexp",
          "string.unquoted",
          "strong",
          "support",
          "support.class",
          "support.constant",
          "support.function",
          "support.other",
          "support.type",
          "support.type.property-name",
          "support.variable",
          "variable",
          "variable.language",
          "variable.name",
          "variable.other",
          "variable.other.readwrite",
          "variable.parameter"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.unicodeHighlight.allowedCharacters": {
    "Ô∏è": true
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/*.g.dart": true,
    "**/*.freezed.dart": true
  },
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "git.autofetch": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "yaml": false,
    "dart": false,
    "javascript": false,
    "rust": true,
    "cpp": false,
    "go": false,
    "python": false,
    "zig": false,
    "ocaml": false,
    "toml": false
  },
  "githubPullRequests.pullBranch": "never",
  "go.toolsManagement.autoUpdate": true,
  "json.format.enable": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "prettier.enable": true,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "prettier.documentSelectors": ["**/*.astro"],
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "redhat.telemetry.enabled": true,
  "search.exclude": {
    "**/.fvm": true
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1",
    "CW_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily": "Geist Mono, SF Mono",
  "terminal.integrated.fontSize": 17,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.editorAssociations": {
    "*.class": "default"
  },
  "workbench.iconTheme": "material-icon-theme",
  "kotlin.languageServer.enabled": false,
  "kotlin.debugAdapter.enabled": false,
  "C_Cpp.intelliSenseEngine": "disabled",
  "security.workspace.trust.untrustedFiles": "open",
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-17",
      "path": "~/Library/Java/JavaVirtualMachines/amazon-corretto-17.jdk/Contents/Home"
    }
  ],
  "java.jdt.ls.java.home": "~/Library/Java/JavaVirtualMachines/amazon-corretto-17.jdk/Contents/Home",
  "java.import.gradle.user.home": "~/.gradle",
  "java.import.gradle.java.home": "${JAVA_HOME}",
  "maven.terminal.useJavaHome": true,
  "kotlin.java.home": "${JAVA_HOME}",
  "react-native-tools.showUserTips": false,
  "update.mode": "start",
  "git.openRepositoryInParentFolders": "never",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 3000,
  "vs-kubernetes": {
    "vs-kubernetes.minikube-show-information-expiration": "2024-04-06T00:03:57.608Z",
    "vscode-kubernetes.helm-path.mac": "/Users/silverhairs/.vs-kubernetes/tools/helm/darwin-arm64/helm"
  },
  "lldb.library": "/Applications/Xcode-15.0.0.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
  "window.commandCenter": true,
  "workbench.layoutControl.enabled": false,
  "liveshare.allowGuestDebugControl": true,
  "liveshare.allowGuestTaskControl": true,
  "cody.autocomplete.enabled": true,
  "copyrightInserter.holder": "Boris Kayi<hello@silverhairs.engineer>",
  "copyrightInserter.license": "apache",
  "errorLens.enabledDiagnosticLevels": ["warning", "info"],
  "lldb.launch.expressions": "native",
  "errorLens.messageEnabled": false,
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": false,
  "diffEditor.renderSideBySide": false,
  "zig.initialSetupDone": true,
  "catppuccin.italicKeywords": false,
  "dart.checkForSdkUpdates": false,
  "zig.path": "~/zig/zig",
  "zig.zls.path": "/Users/silverhairs/Library/Application Support/VSCodium/User/globalStorage/ziglang.vscode-zig/zls_install/zls",
  "window.zoomLevel": -1,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[caddyfile]": {
    "editor.insertSpaces": true,
    "editor.formatOnSave": true
  },
  "cmake.showOptionsMovedNotification": false,
  "editor.inlineSuggest.suppressSuggestions": true,
  "workbench.activityBar.location": "hidden",
"workbench.colorTheme": "Catppuccin Mocha",
  "java.import.gradle.home": "~/.gradle",
  "security.promptForLocalFileProtocolHandling": false,
  "dart.showTodos": true,
  "database-client.autoSync": true,
  "terminal.integrated.allowedLinkSchemes": [
    "file",
    "http",
    "https",
    "mailto",
    "vscode",
    "vscode-insiders",
    "file-line"
  ]
}
